
public class TesteArrays {

	public static void main(String[] args) {
		String[] minhasStrings = { "Melissa", "Barbosa", "Souza" };
		
		for (int i = 0; i < minhasStrings.length; i++) {
			System.out.println(minhasStrings[i]);
		}
		
		int i = 0;
		
		while(i < minhasStrings.length) {
			System.out.println(minhasStrings[i]);
			i++;
		}
	}

}
